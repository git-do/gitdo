      <div id="content-wrap">
        <div id="main">
          <header class="section-header">
            <h2 class="section-title">Open Issues</h2>
          </header>
          
          <ul class="issue-list list">
            <li class="open" ng-repeat="issue in issues">
              <a href="#" class="list-item-header">
                <h3 class="list-heading">{{issue.title}}</h3>
                <span class="issue-date timestamp">created yesterday, 2:33 pm</span>
              </a>
              <div class="issue-details">
                <ul class="issue-meta">
                  <li><span class="issue-meta-label">Branch Name</span> {{issue.branch}}</li>
                  <li><span class="issue-meta-label">File Name</span> {{issue.file}}</li>
                  <li><span class="issue-meta-label">Line Number</span> #{{issue.line}}</li>
                </ul>
                <div class="issue-desc">
                  <span class="issue-meta-label">Issue description</span>
                  <p>{{issue.description}}</p>
                </div>
                <div class="clear"></div>
              </div>
              <div class="issue-footer">
                <a href="#" class="issue-footer-btn btn">Mark as resolved</a>
                <a href="#" class="issue-footer-btn btn btn-primary" target="_blank">View source</a>
              </div>
            </li>
          </ul>
        </div>
      
